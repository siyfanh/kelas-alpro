#include <stdio.h>

int main()
{
    int usia = 18;
    float berat = 54.20;
    double tinggi = 168.72;
    char jenis_kelamin = 'P';

    printf("Usia: %i tahun\n", usia);
    printf("Berat: %.2f Kg\n", berat);
    printf("Tinggi: %.2f cm\n", tinggi);
    printf("Jenis kelamin: %c\n", jenis_kelamin);
    return 0;
}
