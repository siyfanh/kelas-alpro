#include <stdio.h>

void main()
{
    const int ANGKATAN = 23;
    const double KELAS = 47.03;
    const char JENIS_KELAMIN = 'P';

    printf("isi dari konstanta ANGKATAN adalah %i\n", ANGKATAN);
    printf("isi dari konstanta KELAS adalah %.2f\n", KELAS);
    printf("isi dari konstanta JENIS_KELAMIN adalah %c\n", JENIS_KELAMIN);
}
