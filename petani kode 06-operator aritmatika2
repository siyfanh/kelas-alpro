#include <stdio.h>

void main()
{
    int x, y;

    printf("Inputkan nilai x: ");
    scanf("%i", &x);

    printf("Inputkan nilai y: ");
    scanf("%i", &y);

    printf("Hasil x + y: %i\n", x + y);
    printf("Hasil x - y: %i\n", x - y);
    printf("Hasil x * y: %i\n", x * y);
    printf("Hasil x / y: %i\n", x / y);
    printf("Hasil x % y: %i\n", x % y);
}
