#include <stdio.h>

void main()
{
    int x = 12;
    int y = 5;

    printf("x = %i\n", x);
    printf("y = %i\n", y);
    // operator pembanding
    printf("x > y = %i\n", x > y);
    printf("x < y = %i\n", x < y);
    printf("x >= y = %i\n", x >= y);
    printf("x <= y = %i\n", x <= y);
    printf("x == y = %i\n", x == y);
    printf("x != y = %i\n", x != y);
}
